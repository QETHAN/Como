<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<title>Tween</title>
	<script type="text/javascript" src="../../Como.unpack.js?only"></script>
	<script type="text/javascript">
		Como.include('tween/core.js', null, {done:true});
	</script>
</head>
<body>
<div id="test" style="position: absolute; top: 10px; left: px; padding: 10px 20px; border: 1px solid #333;">Test Box</div>

<div style="position:absolute;top:300px;left:10px;">
	<button onclick="test('Quad', 'begin')">Quad  begin</button>
	<button onclick="test('Quad', 'end')">Quad  end</button>
	<button onclick="test('Quad', 'both')">Quad  both</button>

	<button onclick="test('Cubic', 'begin')">Cubic  begin</button>
	<button onclick="test('Cubic', 'end')">Cubic  end</button>
	<button onclick="test('Cubic', 'both')">Cubic  both</button>

	<button onclick="test('Quart', 'begin')">Quart  begin</button>
	<button onclick="test('Quart', 'end')">Quart  end</button>
	<button onclick="test('Quart', 'both')">Quart  both</button>

	<button onclick="test('Quint', 'begin')">Quint  begin</button>
	<button onclick="test('Quint', 'end')">Quint  end</button>
	<button onclick="test('Quint', 'both')">Quint  both</button>

	<button onclick="test('Quart', 'begin')">Sine  begin</button>
	<button onclick="test('Quart', 'end')">Sine  end</button>
	<button onclick="test('Quart', 'both')">Sine  both</button>

	<button onclick="test('Quart', 'begin')">Expo  begin</button>
	<button onclick="test('Quart', 'end')">Expo  end</button>
	<button onclick="test('Quart', 'both')">Expo  both</button>

	<button onclick="test('Quart', 'begin')">Circ  begin</button>
	<button onclick="test('Quart', 'end')">Circ  end</button>
	<button onclick="test('Quart', 'both')">Circ  both</button>

	<button onclick="test('Quart', 'begin')">Elastic  begin</button>
	<button onclick="test('Quart', 'end')">Elastic  end</button>
	<button onclick="test('Quart', 'both')">Elastic  both</button>

	<button onclick="test('Quart', 'begin')">Back  begin</button>
	<button onclick="test('Quart', 'end')">Back  end</button>
	<button onclick="test('Quart', 'both')">Back  both</button>

	<button onclick="test('Quart', 'begin')">Bounce  begin</button>
	<button onclick="test('Quart', 'end')">Bounce  end</button>
	<button onclick="test('Quart', 'both')">Bounce  both</button>
</div>
<script type="text/javascript">
Como.onincludeHandler(function(){
	window.test = function(tw, ea){
		Como('#test').anim().from('top', 10).to('top', 200).from('left', 10).to('left', 500).tween(Como.Tween[tw]).ease(ea).duration(1000).go();
	}
});
</script>
</body>
</html>